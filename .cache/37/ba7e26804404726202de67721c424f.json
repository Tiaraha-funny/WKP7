g the add button from the html\r\n\r\nconst form = document.querySelector('.book');\r\nconst addBtn = document.querySelector('.add');\r\nconst table = document.querySelector('table');\r\n\r\n//function about the adding the book list tin the table\r\n\r\nconst handleAddButtonSubmit = e => {\r\n    console.log('add button');\r\n\r\n    let myNewbooks = [...books];\r\n\r\n    // e.preventDefault();\r\n    const myBook = e.currentTarget.book.value;\r\n\r\n    // if the states is empty return the what we have added before\r\n    if (!myBook) return;\r\n\r\n    // if not, push the adding book\r\n    Book.push(books);\r\n    e.target.reset();\r\n\r\n    //to fire off a custom event  we create our own event called \"itemDateed\"\r\n    table.dispatchEvent(new CustomEvent('tableUpdated'));\r\n}\r\n\r\nconst tableOfTheAddedBookFromTheState = () => {\r\n    console.log(Book);\r\n    const tableHtml = book.map(newBook => `\r\n    <tr>\r\n        <td>${newBook.title}</td>\r\n        <td>${newBook.author}</td>\r\n        <td>${newBook.genre}</td>\r\n        <td>${newBook.pages}</td>\r\n        <td>${newBook.status}</td>\r\n    </tr>\r\n    <button>delete</button>\r\n    `).join('');\r\n    table.innerHTML = tableHtml;\r\n};\r\n\r\n//Adding book by clicking the add button\r\n\r\nform.addEventListener('submit', handleAddButtonSubmit);\r\n\r\n//We listen to our own event, and launch the function table of the added book functon by the listners which we have created\r\ntable.addEventListener('tableUpdated', tableOfTheAddedBookFromTheState);\r\n"},"lineCount":null}},"error":null,"hash":"5a06dd3649cb73acabbdcbdc6fe50ab4","cacheData":{"env":{}}}